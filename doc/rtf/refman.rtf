00166     {\cf20 //--------------------------------------}\par
00167     Polynomial pn1(std::map<int, Monomial>(\{\{1, Monomial(2, 1)\}, \{2, Monomial(2, 2)\}\})), pn3(std::map<int, Monomial>(\{\{1, Monomial(2, 1)\}, \{3, Monomial(3, 3)\}\}));\par
